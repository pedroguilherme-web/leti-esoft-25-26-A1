@startuml
'https://plantuml.com/class-diagram

title US15 Class Diagram

class GetEmployeeListView {
}

class GetEmployeeListController {
  + getEmployeeList(role) : List<EmployeeList>
}

class NurseContainer {
 - nurses : List<NurseUser>
 + getNurseList() : List<NurseUser>
}

class ReceptionistContainer {
 receptionists : List<ReceptionistUser>
 + getReceptionistList(receptionists)
}

class Person {
  # name: String
  # phone_number: int
  # email: String
}

class ReceptionistUser {
  - receptionist_id : int
}

class NurseUser {
  - nurse_id: int
}

GetEmployeeListView ..> GetEmployeeListController : "uses >"
GetEmployeeListController ..> NurseContainer : "uses >"
GetEmployeeListController ..> ReceptionistContainer : "uses >"
GetEmployeeListController ..> EmployeeList : "creates >"
NurseContainer "    1" -- "0..*        " NurseUser : "stores the information of >"
ReceptionistContainer "1" -- "0..*" ReceptionistUser : "stores the information of >"
Person <|-- ReceptionistUser
Person <|-- NurseUser

@enduml